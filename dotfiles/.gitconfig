[user]
    name = Massoud Boorboor
    email = boorboor@gmail.com
    signingkey = 3236520BC1654D472D5D5E28068CFD621D4263B1
[push]
    default = simple
[alias]
    s = status --short
    f = fetch --all --prune --progress --tags
    l = log --pretty=oneline --graph --abbrev-commit -20
    d = diff
    co = checkout
    ll = log --pretty=format:'%h || %G? || %ar || %aN || %s'
    lf = log --pretty=format:'%h %an %s ' --name-status
    search = log -S
    conflict = diff --name-only --diff-filter=U
    unstage = reset HEAD --
[core]
    whitespace = trailing-space,space-before-tab
    pager = less -r
    editor = nvim
    pager = less -F -X
[color]
    ui = true
[remote "origin"]
    prune = true
[commit]
    gpgsign = true
[merege]
    tool = vimdiff
    conflictstyle = diff3
    prompt = false
[pull]
    rebase = true
[credential]
    helper = cache --timeout 30000
[diff]
    tool = vimdiff
    submodule = log
[apply]
    whitespace = fix
[protocol]
    version = 2
[status]
    submodulesummary = 1
